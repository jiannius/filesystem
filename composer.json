{
    "name": "jiannius/filesystem",
    "description": "Jiannius Filesystem",
    "type": "library",
    "require": {
        "doctrine/dbal": "^3.6",
        "intervention/image": "^3",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.0",
        "symfony/http-client": "^6.0",
        "laravel/pail": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jiannius\\Filesystem\\": "src/"
        }
    },
    "authors": [
        {
            "name": "TJ",
            "email": "tj@jiannius.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Jiannius\\Filesystem\\FilesystemServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": false,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
